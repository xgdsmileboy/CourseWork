<?xml version="1.0" encoding="UTF-8"?>
 <web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>SocialNetwork</display-name>
  <servlet> 
    <servlet-name>FileUpload</servlet-name>
    <servlet-class>com.action.FileUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileUpload</servlet-name>
    <url-pattern>/fileupload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUpload</servlet-name>
    <url-pattern>/AcdemicDean/EnrolmentInfo/fileupload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUpload</servlet-name>
    <url-pattern>/AcdemicDean/Elective/fileupload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUpload</servlet-name>
    <url-pattern>/AcdemicDean/AchievementManagement/fileupload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FileDownload</servlet-name>
    <servlet-class>com.action.FileDownload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileDownload</servlet-name>
    <url-pattern>/filedownload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileDownload</servlet-name>
    <url-pattern>/AcdemicDean/EnrolmentInfo/filedownload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileDownload</servlet-name>
    <url-pattern>/AcdemicDean/Elective/filedownload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Account</servlet-name>
    <servlet-class>com.action.Account</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Account</servlet-name>
    <url-pattern>/account</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Student</servlet-name>
    <servlet-class>com.action.Student</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Student</servlet-name>
    <url-pattern>/Student/StudentInfo/student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Student</servlet-name>
    <url-pattern>/Student/Elective/student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Student</servlet-name>
    <url-pattern>/Student/Declaration/student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Student</servlet-name>
    <url-pattern>/Student/AchievementInfo/student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Student</servlet-name>
    <url-pattern>/Student/EvalCourse/student</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AcdemicDean</servlet-name>
    <servlet-class>com.action.AcdemicDean</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AcdemicDean</servlet-name>
    <url-pattern>/AcdemicDean/EnrolmentInfo/acdemicDean</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AcdemicDean</servlet-name>
    <url-pattern>/AcdemicDean/Elective/acdemicDean</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AcdemicDean</servlet-name>
    <url-pattern>/AcdemicDean/Declaration/acdemicDean</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AcdemicDean</servlet-name>
    <url-pattern>/AcdemicDean/Notification/acdemicDean</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AcdemicDean</servlet-name>
    <url-pattern>/AcdemicDean/EvalCourse/acdemicDean</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AcdemicDean</servlet-name>
    <url-pattern>/AcdemicDean/AchievementManagement/acdemicDean</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Admin</servlet-name>
    <servlet-class>com.action.Admin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Admin</servlet-name>
    <url-pattern>/Admin/AcdemicDeanInfo/admin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Admin</servlet-name>
    <url-pattern>/Admin/StudentManagement/admin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Admin</servlet-name>
    <url-pattern>/Admin/CourseManagement/admin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Admin</servlet-name>
    <url-pattern>/Admin/AchievementManagement/admin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Admin</servlet-name>
    <url-pattern>/Admin/NotificationManagement/admin</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>com.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>